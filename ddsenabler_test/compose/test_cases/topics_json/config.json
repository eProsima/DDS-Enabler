          "entityType": "Camera",
          "entityId": "urn:ngsi-ld:camera:cam1",
          "attribute": "shutterSpeed"
        },
        "P2": {
          "entityType": "Arm",
          "entityId": "urn:ngsi-ld:arm:arm1",
          "attribute": "armReach"
        }
      }
    }
  }
